{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"eslint:recommended"
	],
	"globals": {
		"wp": "writable"
	},
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["editorconfig"],
	"rules": {
		"no-console": 1,
		"no-unused-vars": 1
	},
	"settings": {
		"react": {
			"version": "detect"
		}
	}
}
